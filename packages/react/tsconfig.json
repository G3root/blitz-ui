{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2020",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
