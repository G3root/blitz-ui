    "prepublishOnly": "pnpm run build",
    "build-generate": "tsup scripts/__generate.ts -d scripts --minify",
    "run-generate": "node scripts/__generate.js"
  },
  "devDependencies": {
    "@radix-ui/colors": "^0.1.8",
    "fs-extra": "^10.0.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1"
